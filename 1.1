def fill_array_a():
    arr = [[0] * 10 for _ in range(10)]
    for i in range(10):
        arr[i][i] = i
    return arr

def fill_array_b():
    arr = [[0] * 10 for _ in range(10)]
    for i in range(10):
        for j in range(10):
            arr[i][j] = i * 10 + j + 1
    return arr

def fill_array_c():
    arr = [[0] * 10 for _ in range(10)]
    for i in range(10):
        for j in range(10):
            if j == i + 1:
                arr[i][j] = 1
            elif j == i+2:
                arr[i][j] = 2
            elif j == i+3:
                arr[i][j] = 3
            elif j == i+4:
                arr[i][j] = 4
            elif j == i+5:
                arr[i][j] = 5
            elif j == i+6:
                arr[i][j] = 6
            elif j == i+7:
                arr[i][j] = 7
            elif j == i+8:
                arr[i][j] = 8
            elif j == i+9:
                arr[i][j] = 9
    return arr

array_a = fill_array_a()
array_b = fill_array_b()
array_c = fill_array_c()

for row in array_a:
    print(row)
print('\n')

for row in array_b:
    print(row)
print('\n')

for row in array_c:
    print(row)
    
